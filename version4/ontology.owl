        <rdfs:comment>A verification process confirming the authenticity, completeness, and validity of submitted identification and financial documents</rdfs:comment>
        <rdfs:label>DocumentCheck</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#FX -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#FX">
