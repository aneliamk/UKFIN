        <rdfs:comment>Legal document that records the formal terms and conditions of some contract</rdfs:comment>
        <rdfs:label>ContractDocument</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#CreditCardAccount -->


