        <rdfs:label>Certificate</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#ContractDocument -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#ContractDocument">
