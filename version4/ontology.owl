        <rdfs:label>FixedAmortisation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#FixedInterest -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#FixedInterest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Interest"/>
        <rdfs:comment>Interest with a rate that remains constant over an agreed period, or for the entire loan term</rdfs:comment>
        <rdfs:label>FixedInterest</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Guarantor -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Guarantor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Role"/>
