        <rdfs:label>CreditCardAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Currency -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Currency">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#FinancialInstrument"/>
        <rdfs:comment>A system of money in common use, typically issued by a government or monetary authority, serving as a medium of exchange and unit of account</rdfs:comment>
        <rdfs:label>Currency</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#DeathCertificate -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#DeathCertificate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Certificate"/>
        <rdfs:comment>Original document certifying the circumstances of the death, or a certified copy or representation of the ensuing registration of that death</rdfs:comment>
        <rdfs:label>DeathCertificate</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#DefaultNotice -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#DefaultNotice">
