        <rdfs:label>DelinquencyStatus</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#DocumentCheck -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#DocumentCheck">
