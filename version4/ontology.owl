    <!-- https://w3id.org/financialDT/risk#Calculation -->


    <owl:Class rdf:about="https://w3id.org/financialDT/risk#Calculation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/risk#Occurrence"/>
