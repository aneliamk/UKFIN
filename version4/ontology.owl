    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#DebtBalance -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#DebtBalance">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Balance"/>
