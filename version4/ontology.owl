        <rdfs:label>Expense</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#FinancialInstrument -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#FinancialInstrument">
        <rdfs:comment>A contractual arrangement that represents a financial asset to one party and a financial liability or equity instrument to another</rdfs:comment>
        <rdfs:label>FinancialInstrument</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#FinancialRecord -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#FinancialRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Record"/>
        <rdfs:comment>Summary of all previous financial record for a person</rdfs:comment>
        <rdfs:label>FinancialRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#IdentityDocument -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#IdentityDocument">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#LegalDocument"/>
        <rdfs:comment>Any legal document which may be used to verify aspects of a persons identity</rdfs:comment>
        <rdfs:label>IdentityDocument</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Income -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Income">
        <rdfs:comment>Any monetary amount received by an individual or entity from employment, investments, financial products, or other sources within a defined period. In the context of financial risk assessment, Income represents a key financial attribute influencing affordability, repayment capacity, and exposure to credit risk.</rdfs:comment>
        <rdfs:label>Income</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#InsuranceCompany -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#InsuranceCompany">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Organisation"/>
        <rdfs:comment>A financial institution that provides insurance services by underwriting risk in exchange for premiums and compensates policyholders or beneficiaries in the event of specified losses or events</rdfs:comment>
        <rdfs:label>InsuranceCompany</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#InvestmentAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#InvestmentAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#InvestmentOrDepositAccount"/>
        <rdfs:comment>Account that provides a record of deposits of funds and/or securities held at a financial institution</rdfs:comment>
        <rdfs:comment>The typical objectives of an investment account are to achieve long term growth, income or capital preservation from the deposited asset portfolio. Investment accounts are typically not insured</rdfs:comment>
        <rdfs:label>InvestmentAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#InvestmentOrDepositAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#InvestmentOrDepositAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Account"/>
        <rdfs:comment>Account that provides a record of deposits of funds and/or securities held at a financial institution</rdfs:comment>
        <rdfs:label>investmentOrDepositAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#LedgerAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#LedgerAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Account"/>
        <rdfs:comment>Account recorded in a general ledger</rdfs:comment>
        <rdfs:label>LedgerAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#LegalDocument -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#LegalDocument">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Document"/>
        <rdfs:comment>Document specifying the terms of, or provides evidence for, an agreement, attestation, certification, conditions, permissions, and/or decisions of legal persons, government entities, or courts of law drawn up in accordance with certain rules that apply in the relevant jurisdiction(s).</rdfs:comment>
        <rdfs:label>LegalDocument</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Liability -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Liability">
        <rdfs:comment>A financial obligation or debt owed by a borrower or entity to another party, requiring future payments of cash, goods, or services.</rdfs:comment>
        <rdfs:label>Liability</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#LoanAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#LoanAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#LoanOrCreditAccount"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/financialInstitute#hasBalance"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/product#CreditBalance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/product#hasArrears"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/product#Arrears"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Account established by a lender to record the principal, interest, and payment history associated with a specific loan granted to a borrower</rdfs:comment>
        <rdfs:label>LoanAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#LoanOrCreditAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#LoanOrCreditAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Account"/>
        <rdfs:comment>Account established by a lender to record the principal, interest, and payment history associated with a specific loan granted to a borrower</rdfs:comment>
        <rdfs:label>LoanOrCreditAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Location -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Location">
        <rdfs:comment>A geographical place or position that can be identified within a spatial reference system</rdfs:comment>
        <rdfs:label>Location</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#MortgageContract -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#MortgageContract">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#ContractDocument"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/financialInstitute#holdsCollateral"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/product#Asset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/financialInstitute#opens"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#LoanAccount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A contract document detailing the contract for a mortgage</rdfs:comment>
        <rdfs:comment>A mortgage contract will include relation to the person taking the mortgage and the organisation providing it, the upfront payment, the interest rate of the mortgage, the fees in case of a late payment, the arrears allowed before foreclose, the initial start date and the end date, it will also require a collateral with an equal or higher value</rdfs:comment>
        <rdfs:label>MortgageContract</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Notice -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Notice">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Document"/>
        <rdfs:comment>Announcement, communication, intimation, or advance warning of something, usually to allow preparations to be made</rdfs:comment>
        <rdfs:label>Notice</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Organisation -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Organisation">
        <rdfs:comment>A large body of smaller institutes responsible for financial operations</rdfs:comment>
        <rdfs:label>Organisation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Passport -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Passport">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#IdentityDocument"/>
        <rdfs:comment>Formal identity document, issued by a national government, which certifies the identity and nationality of its holder for the purpose of international travel</rdfs:comment>
        <rdfs:label>Passport</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#PaymentAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#PaymentAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Account"/>
        <rdfs:comment>Account used to execute payment transactions, such as the deposit and withdrawal of funds, and to store money available for immediate use through instruments like debit cards, transfers, or direct debits</rdfs:comment>
        <rdfs:label>PaymentAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Person -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Person">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/financialInstitute#hasID"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#IdentityDocument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/financialInstitute#locatedAt"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#Address"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A human being considered as an individual legal or natural person capable of entering into agreements and holding rights or obligations</rdfs:comment>
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Record -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Record">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Document"/>
        <rdfs:comment>Memorialization and objective evidence of activities performed, events occurred, results achieved, or statements made, regardless of its characteristics, media, physical form, or the manner in which it is recorded or stored</rdfs:comment>
        <rdfs:label>Record</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Report -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Report">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Document"/>
        <rdfs:comment>Document that provides a structured description of something, prepared on ad hoc, periodic, recurring, regular, or as required basis</rdfs:comment>
        <rdfs:label>Report</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Stock -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Stock">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#FinancialInstrument"/>
        <rdfs:comment>A financial instrument that signifies ownership interest in a company or entity, representing a claim on part of its assets and earnings</rdfs:comment>
        <rdfs:label>Equity Instrument (Stock)</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#TransactionRecord -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#TransactionRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#Record"/>
        <rdfs:comment>A record for all previous transactions made by a person</rdfs:comment>
        <rdfs:label>TransactionRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Amortisation -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Amortisation">
        <rdfs:comment>A structured process of repaying a loan or debt over time through scheduled periodic payments that cover interest and, typically, portions of the principal amount</rdfs:comment>
        <rdfs:label>Amortisation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#ApplicationStage -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#ApplicationStage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#LifeCycleStage"/>
        <rdfs:comment>The initial phase of a financial product&apos;s life cycle where a customer submits an application and provides required documentation for eligibility and credit assessment</rdfs:comment>
        <rdfs:label>ApplicationStage</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Arrears -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Arrears">
        <rdfs:comment>The state of being behind in fulfilling financial obligations, such as missed or overdue loan or mortgage payments</rdfs:comment>
        <rdfs:label>Arrears</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Asset -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Asset">
        <rdfs:comment>Something of economic value owned or controlled by an entity that is expected to provide future benefit</rdfs:comment>
        <rdfs:label>Asset</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Balance -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Balance">
        <rdfs:comment>A quantified statement of the amount of money held in an account at a given point in time, representing either a credit or debit position</rdfs:comment>
        <rdfs:label>Balance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#BalloonAmortisation -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#BalloonAmortisation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Amortisation"/>
        <rdfs:comment>An amortisation structure that involves regular periodic payments that do not fully repay the principal, followed by a large final payment, known as a balloon payment, at the end of the loan term</rdfs:comment>
        <rdfs:label>BalloonAmortisation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#Borrower -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#Borrower">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/product#hasExpenses"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#Expense"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/product#hasIncome"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#Income"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/financialDT/product#hasLiability"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/financialDT/financialInstitute#Liability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/financialDT/product#Guarantor"/>
        <rdfs:comment>A party that receives money or other assets under a loan agreement and is obligated to repay them under specified terms</rdfs:comment>
        <rdfs:label>Borrower</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#CreditBalance -->


