        <owl:disjointWith rdf:resource="https://w3id.org/financialDT/product#Mortgage"/>
        <rdfs:comment>The exchange of one currency for another, typically conducted through spot or derivative transactions in the foreign exchange market</rdfs:comment>
        <rdfs:label>Foreign Exchange (FX)</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/product#FinancialProduct -->


    <owl:Class rdf:about="https://w3id.org/financialDT/product#FinancialProduct">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/product#Product"/>
        <rdfs:comment>Product provided to consumers and businesses by a depository institution</rdfs:comment>
        <rdfs:label>FinancialProduct</rdfs:label>
    </owl:Class>
    


