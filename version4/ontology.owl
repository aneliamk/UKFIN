        <rdfs:label>DefaultNotice</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#DepositAccount -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#DepositAccount">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#InvestmentOrDepositAccount"/>
        <owl:disjointWith rdf:resource="https://w3id.org/financialDT/financialInstitute#InvestmentAccount"/>
        <rdfs:comment>Account that provides a record of money placed with a depository institution for safekeeping and management</rdfs:comment>
        <rdfs:label>DepositAccount</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Derivative -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Derivative">
        <rdfs:subClassOf rdf:resource="https://w3id.org/financialDT/financialInstitute#FinancialInstrument"/>
        <rdfs:comment>A financial instrument whose value is derived from the value of one or more underlying assets, rates, or indices</rdfs:comment>
        <rdfs:label>Derivative</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#Document -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#Document">
        <rdfs:comment>A document, especially a legal document, may serve to establish one or several facts, and can be relied upon as a proof thereof</rdfs:comment>
        <rdfs:comment>Unitary expression of some realization of an intellectual or artistic work</rdfs:comment>
        <rdfs:label>Document</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/financialDT/financialInstitute#DriverLicence -->


    <owl:Class rdf:about="https://w3id.org/financialDT/financialInstitute#DriverLicence">
